{
   "base": "io.qt.qtwebengine.BaseApp",
   "base-version": "6.6",
   "command": "kiview",
   "desktop-file-name-suffix": " (Beta)",
   "build-options": {
    "env": [
      "QTWEBENGINE_DISABLE_SANDBOX=1"
    ]
  },

   "finish-args": [
      "--share=ipc",
      "--device=dri",
      "--socket=fallback-x11",
      "--socket=wayland",
      "--env=QML_IMPORT_PATH=/app/qml" ,
      "--socket=pulseaudio",
      "--socket=session-bus",
      "--filesystem=host",
      "--filesystem=/tmp",
      "--talk-name=org.freedesktop.Flatpak"

   ],
   "cleanup-commands":[ "/app/cleanup-BaseApp.sh"],
   "id": "com.nyre.kiview",
   "modules": [
      {
         "buildsystem": "cmake-ninja",
         "name": "kiview",
         "sources": [
            {
               "type": "dir",
               "path": "."
            }
         ]
      }

   ],
   "runtime": "org.kde.Platform",
    "runtime-version": "6.6",
   "sdk": "org.kde.Sdk"
}
